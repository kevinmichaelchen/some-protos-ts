{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "acp": "git add . && npm run commit && git push",
    "commit": "git-cz",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "protobufjs": "^6.11.2",
    "long": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "some-protos",
  "author": "Kevin Chen",
  "module": "dist/some-protos.esm.js",
  "size-limit": [
    {
      "path": "dist/some-protos.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/some-protos.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "babel-loader": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "semantic-release": "^17.4.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinmichaelchen/some-protos-ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
